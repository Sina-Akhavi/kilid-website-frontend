ection styles */\n.about {\n  margin-top: 5rem;\n  padding: 2rem;\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0px 4px 5px rgba(0, 0, 0, 0.1); /* Add subtle shadow */\n}\n\n/* Contact section styles */\n.contact {\n  margin-top: 5rem;\n  padding: 2rem;\n  background-color: #eee; /* Light gray for distinction */\n  border-radius: 10px;\n}\n\n.contact h2 {\n  margin-bottom: 1rem;\n  text-align: center; /* Centered heading */\n}\n\n.contact-info {\n  display: flex;\n  align-items: center;\n  margin-bottom: 1rem;\n  border-bottom: 1px solid #ccc; /* Separator between blocks */\n}\n\n.contact-info i {\n  margin-right: 1rem;\n  font-size: 1.4rem; /* Slightly larger icons */\n  color: #333;\n}\n\n.contact-info p {\n  margin-bottom: 0;\n}\n\n.contact-info a {\n  color: #4a90e2; /* Matching link color */\n  text-decoration: none;\n}\n\n.social-media {\n  margin-top: 1rem;\n  text-align: center;\n}\n\n.social-media a {\n  margin-right: 1rem;\n  font-size: 1.5rem; /* Larger social media icons */\n  color: #4a90e2; /* Matching icon color */\n}\n</style>"],"mappings":"AAyEA,eAAe;EACbA,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,eAAe;MAC3BC,OAAO,EAAE,aAAa;MACtBC,OAAO,EAAE,wBAAwB;MACjCC,OAAO,EAAE,kCAAkC;MAC3CC,OAAO,EAAE,gBAAgB;MACzBC,KAAK,EAAE,gBAAgB;MACvBC,KAAK,EAAE;IACT,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}