nexport default {\n  data() {\n    return {\n      agencyName: \"Your Agency Name\",\n      address: \"Agency Address\",\n      phone: \"(555) 555-5555\",\n      email: \"your-email@example.com\",\n    };\n  },\n};\n</script>\n\n<style scoped>\n.agency-profile {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 2rem;\n  font-family: sans-serif;\n  color: #333;\n  background-color: #f2f2f2;\n}\n\n.hero-image {\n  position: relative;\n  width: 100%;\n  height: 300px;\n  overflow: hidden;\n}\n\n.hero-image img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n.overlay {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  color: #fff;\n}\n\n.agency-details {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  width: 80%;\n  margin-top: 2rem;\n}\n\n.about-us {\n  flex-basis: 60%;\n  padding: 1rem;\n  border-radius: 5px;\n  background-color: red;\n}\n\n.logo {\n  width: 150px;\n  margin-bottom: 1rem;\n}\n\n.description {\n  line-height: 1.5;\n}\n\n.contact-info {\n  flex-basis: 40%;\n  padding: 1rem;\n  border-radius: 5px;\n  background-color: #fff;\n}\n\nh2 {\n  margin-bottom: 1rem;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n}\n\nli {\n  margin-bottom: 0.5rem;\n}\n\n/* .fa {\n  font-size: \n} */\n\n</style>"],"mappings":"AAqDA,eAAe;EACbA,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,kBAAkB;MAC9BC,OAAO,EAAE,gBAAgB;MACzBC,KAAK,EAAE,gBAAgB;MACvBC,KAAK,EAAE;IACT,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}